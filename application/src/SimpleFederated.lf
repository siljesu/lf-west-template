target C {
    platform: {
       name: Zephyr,
       board: mimxrt1170_evk_cm7,
       port: /dev/enp4s0f2
    },
    threading: true,
    workers: 2,
    no-compile: true,
    //coordination: decentralized
}


reactor Fed {
    input in: int
    output out: int
}

federated reactor at 192.0.2.1:15047 {
    s = new Fed();
    d = new Fed();

    s.out -> d.in
    d.out -> s.in
}
