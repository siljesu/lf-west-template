target C {
    platform: linux,
    threading: true,
    workers: 2,
    //no-compile: true,
    //coordination: decentralized
}


reactor Fed {
    input in: int
    output out: int
}

federated reactor at 0.0.0.0:15047 {
    s = new Fed();
    d = new Fed();

    s.out -> d.in
    d.out -> s.in
}
